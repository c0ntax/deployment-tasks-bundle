{
    "name": "c0ntax/deployment-tasks-bundle",
    "type": "symfony-bundle",
    "description": "A symfony wrapper around c0ntax/deployment-tasks",
    "minimum-stability": "stable",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Dan Burzynski",
            "email": "contax@phrenetic.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "C0ntax\\DeploymentTasksBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "C0ntax\\DeploymentTasksBundle\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.1",
        "c0ntax/deployment-tasks": "dev-master",
        "knplabs/knp-gaufrette-bundle": "^0.6.0",
        "symfony/config": "^2.8|^3.0|^4.0",
        "symfony/console": "^2.8|^3.0|^4.0",
        "symfony/dependency-injection": "^2.8|^3.0|^4.0",
        "symfony/http-kernel": "^2.8|^3.0|^4.0"
    },
    "require-dev": {
        "pds/skeleton": "^1.0",
        "phpunit/phpunit": "^8.1",
        "symfony/framework-bundle": "^2.8|^3.0|^4.0",
        "symfony/var-dumper": "^2.8|^3.0|^4.0",
        "symfony/yaml": "^2.8|^3.0|^4.0"
    },
    "config": {
        "sort-packages": true
    }
}
